    "require": {
        "php": "^7.1 || ^8.0",
        "composer-plugin-api": "^2.0",
        "kint-php/kint": "^4.1"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "vimeo/psalm": "^4.24",
        "phpunit/phpunit": "^9",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Nibbletech\\Composer\\Plugins\\CustomRules\\": "src/"
        }
    },
    "extra": {
        "class": "Nibbletech\\Composer\\Plugins\\CustomRules\\ComposerRules"
    }
}
